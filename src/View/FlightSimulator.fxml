<Pane prefHeight="419.0" prefWidth="704.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.Controller">
    <Pane fx:id="attributesTableParent" layoutX="7.0" layoutY="27.0" />
